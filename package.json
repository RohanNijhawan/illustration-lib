  "version": "1.0.0",
  "engines": {
	"npm": "6.x",
	"node": "14.x"
  },
  "description": "an open source illustration library",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client-install": "npm install --prefix client",
