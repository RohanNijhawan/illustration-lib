{
	"parser": "babel-eslint",
	"rules": {
		"indent": [1, "tab", { "SwitchCase": 1, "VariableDeclarator": 1 }],
        "no-tabs": 0,
        "react/prop-types": 0,
        "react/jsx-indent": [1, "tab"],
        "react/jsx-indent-props": [1, "tab"],
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"no-multiple-empty-lines": [2, {"max": 99999, "maxEOF": 1}]
	}
}